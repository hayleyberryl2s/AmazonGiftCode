    "keywords": ["Laravel", "AmazonGiftCode", "Amazon", "GiftCard", "AGCOD", "Incentives API", "Amazon Incentives API"],
    "require": {
        "illuminate/support": "~5|^6.0|^7.0|^8.0|^9.0|^10.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
